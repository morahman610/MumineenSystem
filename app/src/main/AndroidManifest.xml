<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mumineen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.Ranking.RankingActivity"></activity>
        <activity android:name=".View.Events.EventsActivity" />
        <activity android:name=".View.LeagueActivity" />
        <activity
            android:name=".UI.ui.RosterActivity"
            android:label="@string/title_activity_league"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".View.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>